{
  "manifest_version": 3,
  "name": "key-sniper",
  "version": "1.0.0",
  "description": "Magic sniper",
  "action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./content_scripts/test.js"]
    }
  ],
  "permissions": [
    "storage"
  ]
}